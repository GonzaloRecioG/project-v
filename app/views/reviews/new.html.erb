<h1><%="Review " + @experience.name %></h1>
<p>Add a review and help other users.</p>

<%= form_for [@experience, @review] do |f| %>
  <%= f.label :comment %>
  <%= f.text_field :comment %>
  <%= f.label :rating %>
  <%= f.select :rating, Review::RATINGS, {pt: "Rate it!"}  %>
  <%= f.submit "Submit" %>

<% end %>

